pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                ls -lrt
                pwd
sh '''
echo 'shell started'
pwd
ls -lrt
'''
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
